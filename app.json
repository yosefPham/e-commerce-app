{
  "expo": {
    "name": "e-commerce-app",
    "slug": "e-commerce-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/image/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/image/backgroundlogin.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/image/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.kientruchanoi",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./app/assets/image/icon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "3dc11c63-e2de-4aca-8a13-42dfacd7750f"
      }
    }
  },
  "name": "E-commerce",
  "displayName": "E-commerce"
}
